generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "sqlite"
}

model Setting {
  key        String  @id
  value      String?
  jsonSchema String?
}

model Message {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  request   String
  response  String
  tokens    Int
}
